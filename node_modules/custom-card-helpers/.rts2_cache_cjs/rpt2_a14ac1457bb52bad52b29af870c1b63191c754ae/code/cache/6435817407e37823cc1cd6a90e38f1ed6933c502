e\";\r\n        case \"vibration\":\r\n            return activated ? \"hass:crop-portrait\" : \"hass:vibrate\";\r\n        case \"window\":\r\n            return activated ? \"hass:window-closed\" : \"hass:window-open\";\r\n        default:\r\n            return activated ? \"hass:radiobox-blank\" : \"hass:checkbox-marked-circle\";\r\n    }\r\n};\r\n//# sourceMappingURL=binary_sensor_icon.js.map","map":"{\"version\":3,\"file\":\"binary_sensor_icon.js\",\"sourceRoot\":\"\",\"sources\":[\"../../src/binary_sensor_icon.ts\"],\"names\":[],\"mappings\":\"AAEA,yDAAyD;AAEzD,MAAM,CAAC,MAAM,gBAAgB,GAAG,CAAC,KAAiB,EAAE,EAAE;IACpD,MAAM,SAAS,GAAG,KAAK,CAAC,KAAK,IAAI,KAAK,CAAC,KAAK,KAAK,KAAK,CAAC;IACvD,QAAQ,KAAK,CAAC,UAAU,CAAC,YAAY,EAAE;QACrC,KAAK,SAAS;YACZ,OAAO,SAAS,CAAC,CAAC,CAAC,cAAc,CAAC,CAAC,CAAC,sBAAsB,CAAC;QAC7D,KAAK,MAAM;YACT,OAAO,SAAS,CAAC,CAAC,CAAC,kBAAkB,CAAC,CAAC,CAAC,gBAAgB,CAAC;QAC3D,KAAK,cAAc;YACjB,OAAO,SAAS,CAAC,CAAC,CAAC,yBAAyB,CAAC,CAAC,CAAC,qBAAqB,CAAC;QACvE,KAAK,MAAM;YACT,OAAO,SAAS,CAAC,CAAC,CAAC,kBAAkB,CAAC,CAAC,CAAC,gBAAgB,CAAC;QAC3D,KAAK,aAAa;YAChB,OAAO,SAAS,CAAC,CAAC,CAAC,aAAa,CAAC,CAAC,CAAC,kBAAkB,CAAC;QACxD,KAAK,KAAK,CAAC;QACX,KAAK,OAAO,CAAC;QACb,KAAK,SAAS,CAAC;QACf,KAAK,QAAQ,CAAC;QACd,KAAK,OAAO;YACV,OAAO,SAAS,CAAC,CAAC,CAAC,mBAAmB,CAAC,CAAC,CAAC,YAAY,CAAC;QACxD,KAAK,MAAM;YACT,OAAO,SAAS,CAAC,CAAC,CAAC,kBAAkB,CAAC,CAAC,CAAC,WAAW,CAAC;QACtD,KAAK,OAAO;YACV,OAAO,SAAS,CAAC,CAAC,CAAC,mBAAmB,CAAC,CAAC,CAAC,mBAAmB,CAAC;QAC/D,KAAK,MAAM;YACT,OAAO,SAAS,CAAC,CAAC,CAAC,WAAW,CAAC,CAAC,CAAC,gBAAgB,CAAC;QACpD,KAAK,UAAU;YACb,OAAO,SAAS,CAAC,CAAC,CAAC,gBAAgB,CAAC,CAAC,CAAC,YAAY,CAAC;QACrD,KAAK,QAAQ;YACX,OAAO,SAAS,CAAC,CAAC,CAAC,WAAW,CAAC,CAAC,CAAC,UAAU,CAAC;QAC9C,KAAK,WAAW;YACd,OAAO,SAAS,CAAC,CAAC,CAAC,mBAAmB,CAAC,CAAC,CAAC,WAAW,CAAC;QACvD,KAAK,SAAS;YACZ,OAAO,SAAS,CAAC,CAAC,CAAC,aAAa,CAAC,CAAC,CAAC,qBAAqB,CAAC;QAC3D,KAAK,MAAM;YACT,OAAO,SAAS,CAAC,CAAC,CAAC,qBAAqB,CAAC,CAAC,CAAC,iBAAiB,CAAC;QAC/D,KAAK,UAAU;YACb,OAAO,SAAS,CAAC,CAAC,CAAC,mBAAmB,CAAC,CAAC,CAAC,WAAW,CAAC;QACvD,